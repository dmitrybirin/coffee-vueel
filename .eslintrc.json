{
	"root": true,
	"env": {
		"node": true
	},
	"parserOptions": { "parser": "babel-eslint", "ecmaVersion": 2018, "sourceType": "module" },
	"plugins": ["prettier"],
	"extends": ["plugin:vue/essential", "eslint:recommended", "plugin:prettier/recommended"],
	"rules": { "prettier/prettier": "error", "no-console":"off" }
}
